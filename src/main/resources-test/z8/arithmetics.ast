i32 __random__;

void printStringLength(u8* str, u8 length) {
	u8 chr;

	while (length > 0) {
		chr = *str;
		printChar(chr);
		length = length - 1;
	}
}

void printUint(i16 number) {
	u8* buffer[20];
	u8 pos;
	i64 remainder;
	u8 digit;

	pos = 20;
	while (true) {
		pos = pos - 1;
		remainder = (i64)(number % 10);
		number = number / 10;
		digit = ((u8)remainder) + 48;
		buffer[(i16)pos] = digit;
		if (number == 0) {
			break;
		}
	}
	printStringLength(&buffer[(i16)pos], 20 - pos);
}

void printIntLf(i16 number) {
	if (number < 0) {
		printChar(45);
		number = -number;
	}
	printUint(number);
	printChar(10);
}

void printChar(u8 chr) {
}

void main() {
	i16 foo;
	i16 bar;
	i16 bazz;
	i16 a;
	i16 b;

	foo = (i16)((4 * 3) + (2 * 5));
	bar = foo * foo;
	foo = 1;
	printIntLf(bar + foo);
	foo = (i16)((1 + 2) * (3 + 4));
	printIntLf(foo);
	printIntLf(bazz);
	a = 1000;
	b = 10;
	printIntLf(a / b);
	printIntLf(a % 256);
	a = 10;
	b = 1;
	printIntLf(a >> b);
	a = 9;
	b = 2;
	printIntLf(a >> b);
	a = 1;
	printIntLf(a << b);
}
