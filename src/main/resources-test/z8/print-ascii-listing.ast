void printNibble(u8 x) {
	x = x & 15;
	if (x > 9) {
		x = x + ((65 - 57) - 1);
	}
	x = x + 48;
	printChar(x);
}

void printHex2(u8 x) {
	printNibble(x / 16);
	printNibble(x);
}

