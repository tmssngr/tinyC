	cast t.13(u8*), t.12(i16)
	addrof t.11, [buffer]
	add t.11, t.11, t.13
	store [t.11], digit
	; 30:3 if number == 0
	const t.15, 0
	equals t.14, number, t.15
	branch t.14, false, @if_5_end
