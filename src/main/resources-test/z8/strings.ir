@printLength:
 Local variables
	var 0: length (i16/2)
	var 1: ptr (u8*/8)
	var 2: t.2 (bool/1)
	var 3: t.3 (u8/1)
	var 4: t.4 (u8/1)
	var 5: t.5 (i16/2)
	var 6: t.6 (i16/2)
	var 7: t.7 (i16/2)
	var 8: t.8 (i16/2)
	; 62 pc
	const length, 0
	move ptr, text
	; 16:2 for *ptr != 0
@for_7:
	load t.3, [ptr]
	const t.4, 0
	notequals t.2, t.3, t.4
	branch t.2, false, @for_7_break
	const t.5, 1
	add length, length, t.5
@for_7_continue:
	cast t.7(i16), ptr(u8*)
	const t.8, 1
	move t.6, t.7
	add t.6, t.6, t.8
	cast ptr(u8*), t.6(i16)
	jump @for_7
@for_7_break:
	call _, printIntLf [length]
@printLength_ret:

@printChar:
	ld   r0, SPH
	ld   r1, SPL
	add  r1, 3
	adc  r0, 0
	ldc  r1, @rr0
	ld   %15, r1
	jp   %0818

