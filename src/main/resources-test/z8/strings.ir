	; 13:2 while length > 0
@while_3:
	const t.4, 0
	gt t.3, length, t.4
	branch t.3, false, @while_3_break
	load chr, [str]
	call _, printChar [chr]
	const t.5, 1
	sub length, length, t.5
	jump @while_3
@while_3_break:
@printStringLength_ret:

@printUint:
