@printString:
 Local variables
	arg 0: str (u8*/8)
	var 1: chr (u8/1)
	var 2: t.2 (bool/1)
	var 3: t.3 (u8/1)
	; 23 pc
	; 2:2 while true
@while_1:
	load chr, [str]
	; 4:3 if chr == 0
	const t.3, 0
	equals t.2, chr, t.3
	branch t.2, false, @if_2_end
	jump @while_1_break
@if_2_end:
	call _, printChar [chr]
	jump @while_1
@while_1_break:
@printString_ret:

@printStringLength:
 Local variables
	arg 0: str (u8*/8)
	arg 1: length (u8/1)
	var 2: chr (u8/1)
	var 3: t.3 (bool/1)
	var 4: t.4 (u8/1)
	var 5: t.5 (u8/1)
