}

void main() {
	u8 chr;
	u8 result;

	chr = 32;
	chars[0] = chr;
	chars[1] = chars[0] + 1;
	chars[(i16)2] = chars[1] + 2;
	result = chars[2];
	printIntLf((i16)result);
}
