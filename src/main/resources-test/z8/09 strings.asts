i32 __random__;
u8* text;

void printString(u8* str) {
	u8 chr;

	while (true) {
		chr = *str;
		if (chr == 0) {
			break;
		}
		printChar(chr);
	}
}

void printStringLength(u8* str, u8 length) {
	u8 chr;

	while (length > 0) {
		chr = *str;
		printChar(chr);
		length = length - 1;
	}
}

void printUint(i16 number) {
	u8* buffer[20];
	u8 pos;
	i64 remainder;
	u8 digit;

	pos = 20;
	while (true) {
		pos = pos - 1;
		remainder = (i64)(number % 10);
		number = number / 10;
		digit = ((u8)remainder) + 48;
		buffer[(i16)pos] = digit;
		if (number == 0) {
			break;
		}
	}
	printStringLength(&buffer[(i16)pos], 20 - pos);
}

void printIntLf(i16 number) {
	if (number < 0) {
		printChar(45);
		number = -number;
	}
	printUint(number);
	printChar(10);
}

