void printLength() {
	i16 length;
	u8* ptr;

	length = 0;
	ptr = text;
	for (; (*ptr) != 0; ptr = (u8*)(((i16)ptr) + 1)) {
		length = length + 1;
	}
	printIntLf(length);
}
