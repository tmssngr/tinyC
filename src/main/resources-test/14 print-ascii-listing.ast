void printNibble(u8 x) {
	x = x & 15;
	if (x > 9) {
		x = x + ((65 - 57) - 1);
	}
	x = x + 48;
	printChar(x);
}

void printHex2(u8 x) {
	printNibble(x / 16);
	printNibble(x);
}

void main() {
	u8 i;
	u8 i;

	printString(" x");
	i = 0;
	for (; i < 16; i = i + 1) {
		if ((i & 7) == 0) {
			printChar(32);
		}
		printNibble(i);
	}
	printChar(10);
	i = 32;
	for (; i < 128; i = i + 1) {
		if ((i & 15) == 0) {
			printHex2(i);
		}
		if ((i & 7) == 0) {
			printChar(32);
		}
		printChar(i);
		if ((i & 15) == 15) {
			printChar(10);
		}
	}
}
